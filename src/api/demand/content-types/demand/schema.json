{
  "kind": "collectionType",
  "collectionName": "demands",
  "info": {
    "singularName": "demand",
    "pluralName": "demands",
    "displayName": "Demanda"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "processing", "allocated", "completed", "cancelled"],
      "default": "pending"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::persona.persona"
    },
    "origin": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address"
    },
    "destination": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address"
    },
    "requestedDate": {
      "type": "datetime"
    },
    "deadlineDate": {
      "type": "datetime"
    },
    "commodityType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::commodity-type.commodity-type"
    },
    "weight": {
      "type": "decimal"
    },
    "volume": {
      "type": "decimal"
    },
    "cargoValue": {
      "type": "decimal"
    },
    "specialRequirements": {
      "type": "json"
    },
    "cargoScheduling": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cargo-scheduling.cargo-scheduling"
    },
    "notes": {
      "type": "text"
    }
  }
}
