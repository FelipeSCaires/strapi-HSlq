    "companyName": {
      "type": "string",
      "required": true
    },
    "tradeName": {
      "type": "string"
    },
    "document": {
      "type": "string",
      "required": true
    },
    "stateRegistration": {
      "type": "string"
    },
    "municipalRegistration": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "logo": {
      "type": "media",
      "allowedTypes": ["images"]
    },
    "anttCode": {
      "type": "string"
    },
    "rntrcCode": {
      "type": "string"
    },
    "fiscalSettings": {
      "type": "json"
    },
    "systemSettings": {
      "type": "json"
    }
  }
