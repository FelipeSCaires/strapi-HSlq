{
  "kind": "collectionType",
  "collectionName": "insurance_averbations",
  "info": {
    "singularName": "insurance-averbation",
    "pluralName": "insurance-averbations",
    "displayName": "Averbação de Seguro"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "policy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::insurance-policy.insurance-policy"
    },
    "trip": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trip.trip"
    },
    "cargoScheduling": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cargo-scheduling.cargo-scheduling"
    },
    "cargoValue": {
      "type": "decimal"
    },
    "premiumValue": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "confirmed", "cancelled"],
      "default": "pending"
    },
    "averbationDate": {
      "type": "datetime"
    },
    "protocol": {
      "type": "string"
    }
  }
}
